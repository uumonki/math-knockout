/* miniprogram/pages/index/index.wxss */

page {
  background: rgb(222,237,255);
  background: linear-gradient( rgba(223,226,233,1),rgba(241,241,241,1));
}

.header {
  background: #f8f8f8;
  box-shadow: 0px 0px 8px 1px rgba(0,0,0,0.1);
  padding: 20px 23px;
  border-top: 1px solid #eee;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-radius: 20px;
  margin: 20px 20px 0 20px;
}

.title {
  font-size: 80rpx;
  line-height: 70rpx;
  margin-right: 15px;
  margin-top: -10rpx;
  letter-spacing: 5rpx;
  font-family: 'english';
}

.pfp {
  flex-basis: 50px;
  height: 50px;
  width: 50px;
  flex-grow: 0;
  flex-shrink: 0;
  border-radius: 25px;
  box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.2);
  overflow: hidden;
}

.body {
  margin: -10px 20px 10px 20px;
}

.section {
  margin-top: 20px;
}

.label {
  margin-left: 5px;
  font-size: 30pt;
  letter-spacing: -1px;
  font-family: 'chinese';
  display: block;
}

.today {
  width: 100%;
  border-radius: 20px;
  box-shadow: 0 0 8px 1px rgba(0, 0, 0, 0.2);
}

.cardsWrapper {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.disabledCard {
  padding: 5px;
  background: white;
  box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.2);
  display: inline-block;
  width: 120px;
  margin: 0px 5px;
  border-radius: 10px;
}

.icon {
  width: 50px;
  display: block;
  margin: 0 auto;
  z-index: 0;
  opacity: 0.6;
}

.subjectLabel{
  display: block;
  text-align: center;
  margin: -5px auto 5px auto;
  font-size: 25rpx;
  opacity: 0.6;
  z-index: 1;
}

.link {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.next {
  flex-basis: 30px;
  height: 30px;
  width: 30px;
  flex-grow: 0;
  flex-shrink: 0;
}

.overlay {
  height: 100vh;
  width: 100vw;
  position: fixed;
  background: rgba(0, 0, 0, 0.5);
  z-index: 90;
}

.rules {
  height: 60%;
  width: calc(100% - 100px);
  min-width: 250px;
  max-width: 500px;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border-radius: 20px;
}

.popup {
  background: white;
  z-index: 91;
  position: fixed;
}

.popupTitle {
  font-family: 'chinese';
  font-size: 40pt;
  letter-spacing: -1.5px;
  margin: 5px 0px 0px 18.5px;
}

.closePopup {
  width: 20px;
  position: absolute;
  top: 12px;
  right: 12px;
  opacity: 0.3;
}

.rulesText {
  margin: -5px 20px 0px 20px;
  width: calc(100% - 40px);
  height: calc(100% - 77px);
  font-size: 10pt;
  line-height: 13pt;
}

.feedback {
  width: 100%;
  height: 347px;
  bottom: 0;
  border-radius: 20px 20px 0 0;
}

.feedbackForm {
  margin: -5px 20px 0px 20px;
  width: calc(100% - 40px);
}

.contact {
  width: calc(100% - 55px) !important;
  display: block;
  margin: auto;
  line-height: 35px;
  height: 35px;
  border-radius: 17.5px;
  background: #eee;
  padding-left: 17.5px
}

.feedbackText {
  width: calc(100% - 70px) !important;
  height: 150px;
  display: block;
  margin: auto;
  border-radius: 17.5px;
  background: #eee;
  padding: 15px 17.5px;
  margin-top: 10px;
  font-size: 11pt;
  line-height: 14pt;
}

.submit {
  height: 35px !important;
  width: 100px !important;
  background: rgb(47,133,252);
  color: white;
  font-size: 12pt;
  padding: 0px;
  line-height: 35px;
  border-radius: 17.5px;
  margin-top: 10px;
}

.b {
  display: inline;
  font-weight: 700;
  background-color: rgb(255, 255, 212);
  border-radius: 5pt;
}

.br {
  display: block;
  width: 100%;
  height: 10px;
}

@font-face {
  font-family: 'english';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'chinese';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

